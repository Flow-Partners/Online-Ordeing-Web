{
  "name": "angular-base-template",
  "version": "1.0.0",
  "description": "A comprehensive Angular base template with authentication, routing, and best practices",
  "scripts": {
    "ng": "ng",
    "start": "ng serve client",
    "start:client": "ng serve client",
    "start:admin": "ng serve admin",
    "start:dev": "ng serve client --configuration development",
    "build": "ng build client",
    "build:client": "ng build client",
    "build:admin": "ng build admin",
    "build:all": "ng build client && ng build admin",
    "build:client:prod": "ng build client --configuration production",
    "build:admin:prod": "ng build admin --configuration production",
    "build:prod": "ng build client --configuration production",
    "watch": "ng build client --watch --configuration development",
    "test": "ng test",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"projects/**/*.{ts,html,scss}\""
  },
  "keywords": [
    "angular",
    "angular-template",
    "angular-starter",
    "angular-boilerplate",
    "typescript",
    "bootstrap",
    "authentication",
    "jwt",
    "spa",
    "frontend",
    "angular19",
    "starter-kit",
    "template",
    "boilerplate"
  ],
